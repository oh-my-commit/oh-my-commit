{
  "name": "oh-my-commit",
  "displayName": "Oh My Commit",
  "private": false,
  "version": "0.8.0",
  "description": "An elegant AI-powered commit message generator for VSCode",
  "author": "markshawn2020",
  "publisher": "markshawn2020",
  "main": "./dist/main/extension.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/oh-my-commit/oh-my-commit"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "git",
    "commit",
    "ai",
    "vscode",
    "productivity"
  ],
  "packageManager": "pnpm@8.10.0",
  "pnpm": {
    "overrides": {
      "typescript": "5.3.3"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "",
    "prepare": "husky install",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "watch": "turbo run watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "tsc -b",
    "clean:modules": "./packages/scripts/clean-modules.sh",
    "changesets": "changeset",
    "version-packages": "changeset version",
    "release": "pnpm build && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "vscode:package": "pnpm build && vsce package --no-dependencies -o dist",
    "vscode:publish": "vsce publish --no-dependencies"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@changesets/config": "^3.0.5",
    "@changesets/git": "^3.0.2",
    "@cs-magic/md-toc": "^0.5.1",
    "@manypkg/get-packages": "^2.2.2",
    "@nrwl/js": "^19.8.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@swc/core": "^1.10.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "autoprefixer": "^10.4.20",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "concurrently": "^9.1.1",
    "cross-env": "^7.0.3",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-file-header": "^0.0.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "figlet": "^1.8.0",
    "glob": "^11.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash-es": "^4.17.21",
    "markdown-toc": "^1.2.0",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "prettier": "^3.4.2",
    "react-refresh": "^0.16.0",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "workspace:*",
    "turbo": "^2.3.3",
    "typedi": "^0.10.0",
    "typescript": "5.3.3",
    "vitepress": "1.5.0",
    "vitest": "2.1.3",
    "webpack": "^5.74.0",
    "zod": "^3.24.1"
  },
  "activationEvents": [
    "onStartupFinished",
    "workspaceContains:.git"
  ],
  "contributes": {
    "views": {
      "ohMyCommit": [
        {
          "type": "webview",
          "id": "ohMyCommit.view",
          "name": "Oh My Commit"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ohMyCommit",
          "title": "Oh My Commit",
          "icon": "$(git-commit)"
        }
      ]
    },
    "commands": [
      {
        "command": "ohMyCommit.quickCommit",
        "title": "Oh My Commit: Quick Commit"
      },
      {
        "command": "ohMyCommit.selectModel",
        "title": "Oh My Commit: Select Model"
      },
      {
        "command": "ohMyCommit.openPreference",
        "title": "Oh My Commit: Open Preference"
      }
    ],
    "configuration": {
      "title": "Oh My Commit",
      "properties": {
        "ohMyCommit.ui.mode": {
          "type": "string",
          "enum": [
            "notification",
            "panel"
          ],
          "enumDescriptions": [
            "弹出通知，支持修改按钮",
            "在面板中显示完整提交界面"
          ],
          "default": "panel",
          "markdownDescription": "提交界面显示模式",
          "order": 1
        },
        "ohMyCommit.basic.uiLanguage": {
          "type": "string",
          "enum": [
            "system",
            "zh_CN",
            "en_US"
          ],
          "enumDescriptions": [
            "跟随系统语言",
            "中文",
            "English"
          ],
          "default": "system",
          "markdownDescription": "界面显示语言",
          "order": 2
        },
        "ohMyCommit.git.commitLanguage": {
          "type": "string",
          "enum": [
            "system",
            "zh_CN",
            "en_US"
          ],
          "enumDescriptions": [
            "跟随系统语言",
            "中文",
            "English"
          ],
          "default": "system",
          "markdownDescription": "Commit 信息语言",
          "order": 3
        },
        "ohMyCommit.model.id": {
          "type": "string",
          "enum": [
            "ohMyCommit.standard"
          ],
          "default": "ohMyCommit.standard",
          "markdownDescription": "选择自动提交（AC）服务的供应商与模型",
          "order": 4
        },
        "ohMyCommit.ai.apiKeys": {
          "type": "object",
          "markdownDescription": "AI 服务提供商的 API 密钥配置",
          "additionalProperties": {
            "type": "string"
          },
          "properties": {
            "openai": {
              "type": "string",
              "default": "",
              "markdownDescription": "OpenAI API 密钥",
              "order": 301
            },
            "anthropic": {
              "type": "string",
              "default": "",
              "markdownDescription": "Anthropic API 密钥",
              "order": 302
            }
          },
          "order": 30
        },
        "ohMyCommit.telemetry.shareLevel": {
          "type": "string",
          "enum": [
            "minimal",
            "basic",
            "full"
          ],
          "enumDescriptions": [
            "仅收集基本错误信息",
            "包含功能使用统计和性能数据",
            "额外包含 AI 生成结果的质量反馈"
          ],
          "default": "basic",
          "markdownDescription": "数据收集级别",
          "order": 51
        }
      }
    }
  }
}
