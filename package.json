{
  "name": "oh-my-commit",
  "displayName": "Oh My Commit",
  "version": "0.0.0",
  "private": true,
  "author": "markshawn2020",
  "repository": {
    "type": "git",
    "url": "https://github.com/oh-my-commit/oh-my-commit"
  },
  "packageManager": "pnpm@8.10.0",
  "pnpm": {
    "overrides": {
      "typescript": "5.3.3"
    }
  },
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "postinstall": "",
    "prepare": "husky install",
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "turbo run dev",
    "watch": "turbo run watch",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:ts": "tsc -b",
    "clean:modules": "./packages/scripts/clean-modules.sh",
    "pkg:update": "changeset",
    "pkg:publish": "changeset version && pnpm build && changeset publish",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:preview": "vitepress preview docs",
    "ext:package": "pnpm -F oh-my-commit package",
    "ext:publish:vscode": "pnpm -F oh-my-commit publish:vscode",
    "ext:publish:ovsx": "pnpm -F oh-my-commit publish:ovsx"
  },
  "devDependencies": {
    "@changesets/cli": "^2.27.11",
    "@changesets/config": "^3.0.5",
    "@changesets/git": "^3.0.2",
    "@cs-magic/md-toc": "^0.5.1",
    "@manypkg/get-packages": "^2.2.2",
    "@nrwl/js": "^19.8.4",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.15",
    "@swc/core": "^1.10.1",
    "@trivago/prettier-plugin-sort-imports": "^5.2.0",
    "@types/figlet": "^1.7.0",
    "@types/jest": "^29.5.14",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.1",
    "@typescript-eslint/eslint-plugin": "6.21.0",
    "@typescript-eslint/parser": "6.21.0",
    "@vscode/vsce": "^3.2.1",
    "autoprefixer": "^10.4.20",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "commander": "^12.1.0",
    "concurrently": "^9.1.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^8.0.0",
    "esbuild": "^0.24.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-file-header": "^0.0.1",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^28.10.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-react": "^7.37.3",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "figlet": "^1.8.0",
    "glob": "^11.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.7.0",
    "lodash-es": "^4.17.21",
    "markdown-toc": "^1.2.0",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "ovsx": "^0.10.1",
    "prettier": "^3.4.2",
    "react-refresh": "^0.16.0",
    "react-refresh-typescript": "^2.0.9",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsup": "workspace:*",
    "turbo": "^2.3.3",
    "typedi": "^0.10.0",
    "typescript": "5.3.3",
    "vitepress": "1.5.0",
    "vitest": "2.1.3",
    "webpack": "^5.74.0",
    "zod": "^3.24.1"
  }
}
