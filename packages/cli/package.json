{
  "name": "@oh-my-commit/cli",
  "version": "0.7.0",
  "description": "Command-line interface for AI-powered commit message generation",
  "bin": {
    "omc": "./dist/index.js"
  },
  "exports": {
    ".": "./dist/index.js",
    "./config": "./dist/config.js"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo",
    "type-check": "tsc --build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "chokidar": "^4.0.3",
    "eslint": "^8.0.0",
    "tsconfig": "*",
    "tsup": "^8.3.5",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@oh-my-commit/shared": "workspace:^",
    "chalk": "^4.x",
    "commander": "^12.0.0",
    "inquirer": "^9.2.12",
    "neverthrow": "^8.1.1",
    "read-package-up": "^11.0.0",
    "simple-git": "^3.22.0",
    "typedi": "^0.10.0"
  },
  "gitHead": "b642fb2921e3f62189d77a6c8f58bf2807f514bc"
}
