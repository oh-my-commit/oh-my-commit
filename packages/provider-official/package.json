    "neverthrow": "^8.1.1",
    "reflect-metadata": "^0.2.1",
    "typedi": "^0.10.0"
  },
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.10.2",
    "tsup": "workspace:*",
