{
  "name": "@oh-my-commit/provider-official",
  "version": "0.25.12",
  "description": "Official AI providers for Oh My Commit - intelligent commit message generation",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "publishConfig": {
