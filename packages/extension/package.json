{
  "name": "@oh-my-commit/extension",
  "version": "0.21.7",
  "description": "VSCode extension for AI-powered commit message generation with customizable providers",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@oh-my-commit/shared": "workspace:^",
    "handlebars": "^4.7.8",
    "lodash": "^4.17.21",
    "outdent": "^0.8.0",
    "reflect-metadata": "^0.2.1",
    "simple-git": "^3.27.0",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^22.10.1",
    "@types/vscode": "^1.80.0",
    "@types/web": "^0.0.135",
    "@vscode/vsce": "^3.2.1",
    "esbuild": "^0.19.11",
    "nodemon": "^2.0.22",
    "tsup": "workspace:*",
    "typescript": "5.3.3",
    "vscode": "^1.1.37"
  }
}
