{
  "name": "@oh-my-commit/shared",
  "version": "0.26.5",
  "description": "Shared utilities and types for Oh My Commit packages",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./common": {
      "types": "./dist/common/index.d.ts",
      "import": "./dist/common/index.js",
      "require": "./dist/common/index.cjs"
    },
    "./server": {
      "types": "./dist/server/index.d.ts",
      "import": "./dist/server/index.js",
      "require": "./dist/server/index.cjs"
    },
    "./server/config": {
      "types": "./dist/server/config.d.ts",
      "import": "./dist/server/config.js",
      "require": "./dist/server/config.cjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist tsconfig.tsbuildinfo .turbo",
    "type-check": "tsc --noEmit"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.0.0",
    "esbuild": "^0.24.2",
    "eslint": "^8.0.0",
    "prettier": "^3.1.0",
    "tsconfig": "^0.0.1",
    "tsup": "workspace:*",
    "typescript": "5.3.3"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.1",
    "chalk": "^5.4.1",
    "handlebars": "^4.7.8",
    "jiti": "^2.4.2",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "neverthrow": "^8.1.1",
    "reflect-metadata": "^0.2.1",
    "simple-git": "^3.27.0",
    "typedi": "^0.10.0",
    "yaml": "^2.6.1",
    "zod": "^3.23.8"
  },
  "gitHead": "b642fb2921e3f62189d77a6c8f58bf2807f514bc"
}
